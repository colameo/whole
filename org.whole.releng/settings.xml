<?xml version="1.0" encoding="UTF-8"?>
<settings>

  <!-- Uncomment this section if you need proxy access.
       Also replace proxy.port.number and proxy.host.name with
       proper values.
  <proxies>
    <proxy>
      <active/>
      <protocol>http</protocol>
      <port>proxy.port.number</port>
      <host>proxy.host.name</host>
      <id/>
    </proxy>
  </proxies>
  -->

  <!-- Replace repository.username and repository.password with
       proper values. -->
  <servers>
    <server>
      <id>WholeRepositoryServer</id>
      <username>repository.username</username>
      <password>repository.password</password>
      <filePermissions>664</filePermissions>
      <directoryPermissions>775</directoryPermissions>
    </server>
  </servers>
</settings> 

